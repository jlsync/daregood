<h1>New attempt</h1>

<% form_for(@attempt) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :challenge_id %><br />
    <%= f.text_field :challenge_id %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', attempts_path %>